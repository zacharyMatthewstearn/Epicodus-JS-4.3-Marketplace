{{!-- {{user-login users=model confirmLogin2="confirmLogin3"}} --}}

{{#link-to 'admin'}}
  <button class="btn btn-primary">Get thee to an admin page!</button>
{{/link-to}}

<div class="row">
  <div class="col-md-10">
    {{#each model.products as |product|}}
      {{product-tile product=product removeFromStock="removeFromStock1"}}
    {{else}}
      <h1>Out of stock for EVERYTHING!</h1>
    {{/each}}
  </div>
  <div id="sidebar" class="col-md-2">
    {{display-cart putBackOnShelf="putBackOnShelf"}}
  </div>
</div>
